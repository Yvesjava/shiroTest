<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appenders>
        <console name="Console" target="SYSTEM_OUT">
            <patternlayout pattern="[%p] %m %n"/>
        </console>
    </appenders>

    <loggers>
        <!--<root level="DEBUG">-->
        <root level="ERROR">
            <appender-ref ref="Console"/>
        </root>
    </loggers>
</configuration>